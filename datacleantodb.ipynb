    "    elif a['attenuation_level'] >= 70.0 and a['attenuation_level'] < 82.0:\n",
    "        return 'Moderate'\n",
    "    elif a['attenuation_level'] >= 82.0:\n",
    "        return 'Aggressive'\n",
    "    else:\n",
    "        'Other'\n",
    "        \n",
    "modifiedjson['attenuation_category'] = modifiedjson.apply(attenCat, axis =1)    "
