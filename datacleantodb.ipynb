   "source": [
    "#function to scale ibu into 3 categories \n",
    "def ibuCat(i):\n",
    "    if i['ibu'] >= 0 and i['ibu'] < 25:\n",
    "        return 'Low'\n",
    "    elif 25 >= i['ibu'] < 38:\n",
    "        return 'Medium'\n",
    "    elif i['ibu'] >=40:\n",
    "        return 'High'\n",
    "    else:\n",
    "        return 'Other'\n",
    "    \n",
    "modifiedjson['ibu_category'] = modifiedjson.apply(ibuCat, axis =1)"
   ]
  },
  {
   "cell_type": "code",
