   "source": [
    "#function to scale abv into 3 categories \n",
    "def abvCat(a):\n",
    "    if a['abv'] >= 0 and a['abv'] < 9:\n",
    "        return 'Low'\n",
    "    elif 9 >= a['abv'] < 16:\n",
    "        return 'Medium'\n",
    "    elif a['abv'] >=16:\n",
    "        return 'High'\n",
    "    else:\n",
    "        return 'Other'\n",
    "\n",
    "modifiedjson['abv_category'] = modifiedjson.apply(abvCat, axis =1)    "
   ]
